{
  "name": "fargatetasksizevalidator",
  "version": "1.0.0",
  "description": "Validates CPU units and Memory for a fargate task, it makes sure the combination is supported by AWS",
  "main": "lib/cpu-memory-validator.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBPlayer/FargateTaskSizeValidator"
  },
  "keywords": [
    "AWS",
    "Fargate",
    "CPU",
    "Memory"
  ],
  "author": "LBPlayer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "10.17.27",
    "jest": "^26.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts"
    ]
  }
}
